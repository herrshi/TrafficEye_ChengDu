<?xml version="1.0" ?>
<configuration>
	<layer>http://[serverip]/arcgis/rest/services/cd_device/MapServer/2</layer>
	<titlefield>desc_</titlefield>
	<idfield>name</idfield>
	<type>XHD</type>
	<linkfield></linkfield>
	<fields all="false">
		<field name="desc_" />
		<field name="name" alias="设备编号" />
	</fields>
	<eventType flag="openDevice" />
	<filterfield>
		<name></name>
		<alias></alias>
	</filterfield>
	<refreshrate>0</refreshrate>
	<query>1=1</query>
	<info>widgets/InfoTemplates/InfoPopupWidget.swf</info>
	<keyfield></keyfield>
	<symbols useinranksymbolwidget="true">
		<symbol keyvalue="default" alias="缺省状态">
			<picturemarkersymbol url="assets/images/device/xhd20.png"
				height="20" width="20" />
		</symbol>
	</symbols>
</configuration>
