	<symbols useinranksymbolwidget="true">
		<symbol keyvalue="1">
			<picturemarkersymbol url="assets/images/planPoint1.png"
				height="20" width="20" />
		</symbol>
		<symbol keyvalue="2">
			<picturemarkersymbol url="assets/images/planPoint2.png"
				height="20" width="20" />
		</symbol>
		<symbol keyvalue="3">
			<picturemarkersymbol url="assets/images/PoliceCar.png"
				height="40" width="40" />
		</symbol>
	</symbols>
</configuration>
