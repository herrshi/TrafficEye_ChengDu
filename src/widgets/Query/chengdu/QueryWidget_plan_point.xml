	<titlefield>des</titlefield>
	<idfield></idfield>
	<type></type>
	<linkfield></linkfield>
	<fields all="false">
		<field name="des" alias="设备描述" />
		<field name="type" alias="设备类型" show="false" />
		<field name="plan" alias="处置措施" />
	</fields>
	<filterfield>
		<name></name>
		<alias></alias>
	</filterfield>
	<refreshrate>0</refreshrate>
