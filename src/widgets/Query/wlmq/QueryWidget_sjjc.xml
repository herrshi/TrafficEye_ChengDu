<?xml version="1.0" ?>
<configuration>
	<layer>http://[serverip]/arcgis/rest/services/wlmq/wlmq_device/MapServer/2</layer>
	<titlefield>desc_</titlefield>
	<idfield>deviceid</idfield>
	<type>SJJC</type>
	<linkfield></linkfield>
	<fields all="false">
		<field name="pos" alias="设备描述" />
		<field name="desc_" alias="检测内容" />
		<field name="deviceid" alias="设备编号" />
		<field name="type_" show="false" />
	</fields>
	<eventType flag="openDevice" />
	<buttons>
		<button>状态查看</button>
	</buttons>
	<refreshrate>0</refreshrate>
	<zoomscale>48000</zoomscale>
	<query>1=1</query>
	<info>widgets/InfoTemplates/InfoPopupWidget.swf</info>
	<keyfield>type_</keyfield>
	<symbols useinranksymbolwidget="true">
		<symbol keyvalue="SJJC" alias="缺省状态">
			<picturemarkersymbol url="assets/images/device/dzjc_gj30.png"
				height="20" width="20" />
		</symbol>
		<symbol keyvalue="SJJC1" alias="发生事件">
			<picturemarkersymbol url="assets/images/sjjc.swf"
				height="20" width="20" />
		</symbol>
	</symbols>

</configuration>
