 <?xml version="1.0" ?>
<configuration>
	<layer>http://[serverip]/arcgis/rest/services/changsha/changsha_device/MapServer/0</layer>
	<titlefield>desc_</titlefield>
	<idfield>Id</idfield>
	<type>VDT</type>
	<linkfield></linkfield>
	<fields all="false">
		<field name="desc_" alias="设备描述" />
		<field name="Id" alias="设备编号" />
		<field name="road" alias="设备位置" />
	</fields>
	<buttons>
		<button>车检器</button>
	</buttons>
	<refreshrate>0</refreshrate>
	<zoomscale>96000</zoomscale>
	<query>1=1</query>
	<info>widgets/InfoTemplates/InfoPopupWidget.swf</info>
	<keyfield></keyfield>
	<symbols maxscale="768000" minscale="192000">
		<symbol keyvalue="default" alias="缺省状态">
			<picturemarkersymbol url="assets/images/icon-vd-s.png"
				height="8" width="8" />
		</symbol>
	</symbols>
	<symbols maxscale="96000" minscale="24000"
		useinranksymbolwidget="true">
		<symbol keyvalue="default" alias="缺省状态">
			<picturemarkersymbol url="assets/images/icon-vd-s.png"
				height="20" width="20" />
		</symbol>
	</symbols>
	<symbols maxscale="12000" minscale="6000">
		<symbol keyvalue="default" alias="缺省状态">
			<picturemarkersymbol url="assets/images/icon-vd-s.png"
				height="30" width="30" />
		</symbol>
	</symbols>
</configuration>
