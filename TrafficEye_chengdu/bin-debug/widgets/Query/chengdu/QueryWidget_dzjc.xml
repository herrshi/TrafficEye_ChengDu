<!--三环管制标牌 -->
<?xml version="1.0" ?>
<configuration>
	<layer>http://[serverip]/arcgis/rest/services/cd_device/MapServer/10
	</layer>
	<titlefield>pos</titlefield>
	<idfield>Id</idfield>
	<type></type>
	<linkfield></linkfield>
	<fields all="false">
		<field name="pos" alias="设备位置" />
		<field name="display" alias="设备信息" />
	</fields>
	<filterfield>
		<name></name>
		<alias></alias>
	</filterfield>
	<refreshrate>0</refreshrate>
	<zoomscale>16000</zoomscale>
	<query>1=1</query>
	<info>widgets/InfoTemplates/InfoPopupWidget.swf</info>
	<keyfield></keyfield>
	<symbols useinranksymbolwidget="true">
		<symbol keyvalue="default">
			<picturemarkersymbol url="assets/images/dzjc.png"
				height="20" width="20" />
		</symbol>
	</symbols>
</configuration>
