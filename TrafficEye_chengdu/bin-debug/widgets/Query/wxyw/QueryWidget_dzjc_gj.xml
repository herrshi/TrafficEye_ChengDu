<?xml version="1.0" ?>
<configuration>
	<relatewidgets>高架电子警察数量</relatewidgets>
	<layer>http://[serverip]/arcgis/rest/services/wx_yw/MapServer/8</layer>
	<titlefield>SDE.JT_GJDZJC_PT.DES</titlefield>
	<idfield>SDE.JT_GJDZJC_PT.DEVICEID</idfield>
	<linkfield></linkfield>
	<fields all="false">
		<field name="SDE.JT_GJDZJC_PT.DES" />
		<field name="SDE.JT_GJDZJC_PT.SUPPLIER" alias="供货厂商" />
		<field name="SDE.JT_GJDZJC_PT.DZJC_ID" alias="设备编号" />
		<field name="SDE.JT_GJDZJC_PT.DEVICEID" show="false" />
		<field name="DEV_STATUS" show="false" />
	</fields>
	<filterfield>
		<name></name>
		<alias></alias>
	</filterfield>
	<refreshrate></refreshrate>
	<query>1=1</query>
	<info>widgets/InfoTemplates/InfoPopupWidget.swf</info>
	<keyfield>DEV_STATUS</keyfield>
	<symbols useinranksymbolwidget="true">
		<symbol keyvalue="0" alias="正常">
			<picturemarkersymbol url="assets/images/wxyw/dzjc_gj20_0.png"
				height="20" width="20" />
		</symbol>
		<symbol keyvalue="1" alias="普通报警">
			<picturemarkersymbol url="assets/images/wxyw/dzjc_gj20_1.png"
				height="20" width="20" />
		</symbol>
		<symbol keyvalue="2" alias="警告报警">
			<picturemarkersymbol url="assets/images/wxyw/dzjc_gj20_2.png"
				height="20" width="20" />
		</symbol>
		<symbol keyvalue="3" alias="严重报警">
			<picturemarkersymbol url="assets/images/wxyw/dzjc_gj20_3.png"
				height="20" width="20" />
		</symbol>
		<symbol keyvalue="4" alias="致命报警">
			<picturemarkersymbol url="assets/images/wxyw/dzjc_gj20_4.png"
				height="20" width="20" />
		</symbol>
		<symbol keyvalue="default" alias="未监控">
			<picturemarkersymbol url="assets/images/wxyw/dzjc_gj20_default.png"
				height="20" width="20" />
		</symbol>
	</symbols>
	<eventType flag="reportDevice" />
</configuration>
