<?xml version="1.0" ?>
<configuration>
	<layer>http://[serverip]/arcgis/rest/services/wlmq/wlmq_device/MapServer/1</layer>
	<titlefield>desc_</titlefield>
	<idfield>deviceid</idfield>
	<type>CCTVC</type>
	<linkfield></linkfield>
	<fields all="false">
		<field name="desc_" alias="设备描述" />
		<field name="deviceid" alias="设备编号" />
		<field name="type" alias="设备类型" />
	</fields>
	<filterfield>
		<name></name>
		<alias></alias>
	</filterfield>
	<refreshrate>0</refreshrate>
	<zoomscale>48000</zoomscale>
	<query>1=1</query>
	<info>widgets/InfoTemplates/InfoPopupWidget.swf</info>
	<keyfield></keyfield>
	<symbols maxscale="768000" minscale="192000">
		<symbol keyvalue="default" alias="缺省状态">
			<picturemarkersymbol url="assets/images/device/cctv8.png"
				height="8" width="8" />
		</symbol>
	</symbols>
	<symbols maxscale="96000" minscale="24000"
		useinranksymbolwidget="true">
		<symbol keyvalue="default" alias="缺省状态">
			<picturemarkersymbol url="assets/images/device/cctv20.png"
				height="20" width="20" />
		</symbol>
	</symbols>
	<symbols maxscale="12000" minscale="6000">
		<symbol keyvalue="default" alias="缺省状态">
			<picturemarkersymbol url="assets/images/device/cctv30.png"
				height="30" width="30" />
		</symbol>
	</symbols>
</configuration>
