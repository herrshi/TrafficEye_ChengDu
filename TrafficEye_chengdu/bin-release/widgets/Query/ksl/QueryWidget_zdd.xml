<?xml version="1.0" ?>
<configuration>
	<relatewidgets>匝道灯数量</relatewidgets>
    <layer>http://[serverip]/arcgis/rest/services/ksl_live_device/MapServer/2</layer>
    <titlefield>SDE_zdd_ksl_二级显示.DEVDESC</titlefield>
    <idfield>SDE_zdd_ksl_二级显示.DEVICEID</idfield>
    <linkfield></linkfield>
    <type>ZDD</type>
    <fields all="false">
        <field name="SDE_zdd_ksl_二级显示.DEVICEID"  alias="设备编号"/>
        <field name="SDE_zdd_ksl_二级显示.DEVDESC"/>
        <field name="FSTR_STATE" show="false"/>
    </fields>
    <filterfield>
        <name></name>
        <alias></alias>
    </filterfield>
    <refreshrate></refreshrate>
    <zoomscale>16000</zoomscale>
    <query>1=1</query>
    <info>widgets/InfoTemplates/InfoPopupWidget.swf</info>
    <keyfield>FSTR_STATE</keyfield>
    <symbols useinranksymbolwidget="true" >    	
    		<symbol keyvalue="opened" alias="打开">
 	   		<picturemarkersymbol url="assets/images/icon-zdd-open.png" height="20" width="20"/>
   	 	</symbol>
   	 	<symbol keyvalue="closed" alias="关闭">
   	 		<picturemarkersymbol url="assets/images/icon-zdd-close.png" height="20" width="20"/>
    		</symbol>
    		<symbol keyvalue="black" alias="黑屏">
    			<picturemarkersymbol url="assets/images/icon-zdd-black.png" height="20" width="20"/>
    		</symbol>
   	 	<symbol keyvalue="unknown" alias="未知">
    			<picturemarkersymbol url="assets/images/icon-zdd-unknown.png" height="20" width="20"/>
   	 	</symbol>
  	  	<symbol keyvalue="default" alias="其他">
   	 		<picturemarkersymbol url="assets/images/icon-zdd-unknown.png" height="20" width="20" />
   	 	</symbol>	
    </symbols>
</configuration>
