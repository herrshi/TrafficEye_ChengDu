<?xml version="1.0" ?>
<configuration>
	<relatewidgets>信号机数量</relatewidgets>
    <layer>http://[serverip]/arcgis/rest/services/wuxi/wx_yw/MapServer/5</layer>
    <titlefield>SDE_JT_XHJ_PT_PGIS.DES</titlefield>
    <idfield>SDE_JT_XHJ_PT_PGIS.DEVICEID</idfield>
    <linkfield></linkfield>
    <fields all="false">
    	<field name="SDE_JT_XHJ_PT_PGIS.DES"/>
        <field name="SDE_JT_XHJ_PT_PGIS.SUPPLIER" alias="供货厂商"/>
        <field name="SDE_JT_XHJ_PT_PGIS.XHJ_ID" alias="设备编号"/>
        <field name="SDE_JT_XHJ_PT_PGIS.DEVICEID" show="false"/>
        <field name="DEV_STATUS" show="false"/>
    </fields>
    <filterfield>
        <name></name>
        <alias></alias>
    </filterfield>
    <refreshrate></refreshrate>
    <query>1=1</query>
    <info>widgets/InfoTemplates/InfoPopupWidget.swf</info>
    <keyfield>DEV_STATUS</keyfield>
    <symbols useinranksymbolwidget="true" >    	
    	<symbol keyvalue="0" alias="正常">
    		<picturemarkersymbol url="assets/images/wxyw/xhj20_0.png" height="20" width="20"/>
    	</symbol>
    	<symbol keyvalue="1" alias="普通报警">
    		<picturemarkersymbol url="assets/images/wxyw/xhj20_1.png" height="20" width="20"/>
    	</symbol>
    	<symbol keyvalue="2" alias="警告报警">
    		<picturemarkersymbol url="assets/images/wxyw/xhj20_2.png" height="20" width="20"/>
    	</symbol>
    	<symbol keyvalue="3" alias="严重报警">
    		<picturemarkersymbol url="assets/images/wxyw/xhj20_3.png" height="20" width="20"/>
    	</symbol>
    	<symbol keyvalue="4" alias="致命报警">
    		<picturemarkersymbol url="assets/images/wxyw/xhj20_4.png" height="20" width="20"/>
    	</symbol>
    	<symbol keyvalue="default" alias="未监控">
    		<picturemarkersymbol url="assets/images/wxyw/xhj20_default.png" height="20" width="20" />
    	</symbol>
    </symbols>
    <eventType flag="reportDevice"/>
</configuration>
