<?xml version="1.0" ?>
<configuration>
	<relatewidgets>真实图交通状态图例</relatewidgets>
	<layer>http://[serverip]/arcgis/rest/services/chengdu/cd_device/MapServer/4</layer>
	<titlefield>des</titlefield>
	<idfield>deviceid</idfield>
	<type>QBB</type>
	<linkfield></linkfield>
	<fields all="false">
		<field name="des" alias="设备描述" />
		<field name="deviceid" alias="设备编号" />
	</fields>
	<filterfield>
		<name></name>
		<alias></alias>
	</filterfield>
	<refreshrate>0</refreshrate>
	<zoomscale>16000</zoomscale>
	<query>1=1</query>
	<info>widgets/InfoTemplates/InfoPopupWidget.swf</info>
	<keyfield></keyfield>
	<symbols maxscale="768000" minscale="384000">
		<symbol keyvalue="default" alias="缺省状态">
			<picturemarkersymbol url="assets/images/device/qbb8.png"
				height="8" width="8" />
		</symbol>
	</symbols>
	<symbols maxscale="192000" minscale="24000"
		useinranksymbolwidget="true">
		<symbol keyvalue="default" alias="缺省状态">
			<picturemarkersymbol url="assets/images/device/qbb20.png"
				height="20" width="20" />
		</symbol>
	</symbols>
	<symbols maxscale="12000" minscale="6000">
		<symbol keyvalue="default" alias="缺省状态">
			<picturemarkersymbol url="assets/images/device/qbb30.png"
				height="30" width="30" />
		</symbol>
	</symbols>
	<eventType flag="openDevice" />
</configuration>
