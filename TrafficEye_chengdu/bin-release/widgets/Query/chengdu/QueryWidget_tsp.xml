<?xml version="1.0" ?>
<configuration>
	<layer>http://[serverip]/arcgis/rest/services/cd_device/MapServer/6</layer>
	<titlefield>des</titlefield>
	<idfield>type</idfield>
	<type>TSP</type>
	<linkfield></linkfield>
	<fields all="false">
		<field name="des" alias="设备描述" />
		<field name="type" alias="设备类型" show="false" />
	</fields>
	<filterfield>
		<name></name>
		<alias></alias>
	</filterfield>
	<refreshrate>0</refreshrate>
	<zoomscale>16000</zoomscale>
	<query>1=1</query>
	<info>widgets/InfoTemplates/InfoPopupWidget.swf</info>
	<keyfield></keyfield>
	<symbols maxscale="768000" minscale="192000">
		<symbol keyvalue="default" alias="缺省状态">
			<picturemarkersymbol url="assets/images/device/tsp8.png"
				height="8" width="8" />
		</symbol>
	</symbols>
	<symbols maxscale="96000" minscale="24000"
		useinranksymbolwidget="true">
		<symbol keyvalue="default" alias="缺省状态">
			<picturemarkersymbol url="assets/images/device/tsp20.png"
				height="20" width="20" />
		</symbol>
	</symbols>
	<symbols maxscale="12000" minscale="6000">
		<symbol keyvalue="default" alias="缺省状态">
			<picturemarkersymbol url="assets/images/device/tsp30.png"
				height="30" width="30" />
		</symbol>
	</symbols>
</configuration>
