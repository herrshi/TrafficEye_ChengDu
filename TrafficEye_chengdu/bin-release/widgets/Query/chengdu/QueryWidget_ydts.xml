<!--远端提示-->
<?xml version="1.0" ?>
<configuration>
    <layer>http://[serverip]/arcgis/rest/services/cd_device/MapServer/14</layer>
    <titlefield>type</titlefield>
    <idfield>Id</idfield>
    <type></type>
    <linkfield></linkfield>
    <fields all="false">
        <field name="type"/>
        <field name="Name" alias="标牌位置"/>
        <field name="Id" show="false"/>
    </fields>
    <filterfield>
        <name></name>
        <alias></alias>
    </filterfield>
    <refreshrate>0</refreshrate>
    <zoomscale>16000</zoomscale>
    <query>1=1</query>
    <info>widgets/InfoTemplates/InfoPopupWidget.swf</info>
    <keyfield></keyfield>
    <symbols maxscale="768000" minscale="192000">
    	<symbol keyvalue="default" alias="缺省状态">
    		<picturemarkersymbol url="assets/images/device/tsp8.png" height="8" width="8" />
    	</symbol>
    </symbols>
    <symbols maxscale="96000" minscale="24000" useinranksymbolwidget="true">
    	<symbol keyvalue="default" alias="缺省状态">
    		<picturemarkersymbol url="assets/images/device/tsp20.png" height="20" width="20" />
    	</symbol>
    </symbols>
    <symbols maxscale="12000" minscale="6000">
    	<symbol keyvalue="default" alias="缺省状态">
    		<picturemarkersymbol url="assets/images/device/tsp30.png" height="30" width="30" />
    	</symbol>
    </symbols>
</configuration>
