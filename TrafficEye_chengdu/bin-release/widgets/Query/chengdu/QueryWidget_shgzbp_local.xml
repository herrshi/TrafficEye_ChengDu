<!--三环管制标牌 -->
<?xml version="1.0" ?>
<configuration>
	<relatewidgets>三环管制标牌数量</relatewidgets>
	<layer>http://[serverip]/arcgis/rest/services/chengdu/cd_device/MapServer/9</layer>
	<titlefield>Name</titlefield>
	<idfield>Id</idfield>
	<type>GZD</type>
	<linkfield></linkfield>
	<fields all="false">
		<field name="Name" alias="标牌位置" />
		<field name="desc_" alias="标牌信息" />
		<field name="type" show="false" />
		<field name="Id" show="false" />
	</fields>
	<filterfield>
		<name></name>
		<alias></alias>
	</filterfield>
	<refreshrate>0</refreshrate>
	<zoomscale>16000</zoomscale>
	<query>1=1</query>
	<info>widgets/InfoTemplates/InfoPopupWidget.swf</info>
	<keyfield>type</keyfield>
	<symbols useinranksymbolwidget="true">
		<symbol keyvalue="一级管制点">
			<picturemarkersymbol url="assets/images/construction-green.png"
				height="20" width="20" />
		</symbol>
		<symbol keyvalue="二级管制点">
			<picturemarkersymbol url="assets/images/construction-yellow.png"
				height="20" width="20" />
		</symbol>
		<symbol keyvalue="三级管制点">
			<picturemarkersymbol url="assets/images/construction-red.png"
				height="20" width="20" />
		</symbol>
		<symbol keyvalue="三环管制标牌" alias="主要固定标牌">
			<picturemarkersymbol url="assets/images/shgzbp.png"
				height="12" width="12" />
		</symbol>
	</symbols>
</configuration>
