<?xml version="1.0" ?>
<configuration>
    <layer>http://[serverip]/arcgis/rest/services/cd_device/MapServer/2</layer>
    <titlefield>desc_</titlefield>
    <idfield>name</idfield>
    <type>XHD</type>
    <linkfield></linkfield>
    <fields all="false">
        <field name="desc_"/>
        <field name="name" alias="设备编号"/>
    </fields>
    <filterfield>
        <name></name>
        <alias></alias>
    </filterfield>
    <refreshrate>0</refreshrate>
    <query>1=1</query>
    <info>widgets/InfoTemplates/InfoPopupWidget.swf</info>   
    <keyfield></keyfield>
    <symbols maxscale="768000" minscale="96000">
    	<symbol keyvalue="default" alias="缺省状态">
    		<picturemarkersymbol url="assets/images/device/xhd8.png" height="8" width="8" />
    	</symbol>
    </symbols>
    <symbols maxscale="48000" minscale="24000" useinranksymbolwidget="true">
    	<symbol keyvalue="default" alias="缺省状态">
    		<picturemarkersymbol url="assets/images/device/xhd20.png" height="20" width="20" />
    	</symbol>
    </symbols>
    <symbols maxscale="12000" minscale="6000">
    	<symbol keyvalue="default" alias="缺省状态">
    		<picturemarkersymbol url="assets/images/device/xhd30.png" height="30" width="30" />
    	</symbol>
    </symbols>
</configuration>
