<!--诱导板-->
<?xml version="1.0" ?>
<configuration>
    <layer>http://[serverip]/arcgis/rest/services/changsha/changsha_device/MapServer/1</layer>
    <titlefield>desc_</titlefield>
    <idfield>deviceid</idfield>
    <type>MB</type>
    <linkfield></linkfield>
    <fields all="false">
        <field name="desc_" alias="设备描述"/>
        <field name="deviceid" alias="设备编号"/>
        <field name="type_" show="false"/>
    </fields>
    <buttons>
		<button>诱导板</button>
	</buttons>
    <refreshrate>0</refreshrate>
    <zoomscale>48000</zoomscale>
    <query>1=1</query>
    <info>widgets/InfoTemplates/InfoPopupWidget.swf</info>
    <keyfield>type_</keyfield>
    <symbols maxscale="768000" minscale="192000">
    	<symbol keyvalue="default" alias="缺省状态">
    		<picturemarkersymbol url="assets/images/icon-m-s.png" height="8" width="8" />
    	</symbol>
    	<symbol keyvalue="m" alias="M板">
    		<picturemarkersymbol url="assets/images/icon-m-s.png" height="8" width="8" />
    	</symbol>
    	<symbol keyvalue="l" alias="L板">
    		<picturemarkersymbol url="assets/images/icon-l-s.png" height="8" width="8" />
    	</symbol>
    	<symbol keyvalue="a" alias="A板">
    		<picturemarkersymbol url="assets/images/icon-a-s.png" height="8" width="8" />
    	</symbol>
    	<symbol keyvalue="c" alias="C板">
    		<picturemarkersymbol url="assets/images/icon-c-s.png" height="8" width="8" />
    	</symbol>
    </symbols>
    <symbols maxscale="96000" minscale="24000" useinranksymbolwidget="true">
    	<symbol keyvalue="default" alias="缺省状态">
    		<picturemarkersymbol url="assets/images/icon-m-s.png" height="20" width="20" />
    	</symbol>
    	<symbol keyvalue="m" alias="M板">
    		<picturemarkersymbol url="assets/images/icon-m-s.png" height="20" width="20" />
    	</symbol>
    	<symbol keyvalue="l" alias="L板">
    		<picturemarkersymbol url="assets/images/icon-l-s.png" height="20" width="20" />
    	</symbol>
    	<symbol keyvalue="a" alias="A板">
    		<picturemarkersymbol url="assets/images/icon-a-s.png" height="20" width="20" />
    	</symbol>
    	<symbol keyvalue="c" alias="C板">
    		<picturemarkersymbol url="assets/images/icon-c-s.png" height="20" width="20" />
    	</symbol>
    </symbols>
    <symbols maxscale="12000" minscale="6000">
    	<symbol keyvalue="default" alias="缺省状态">
    		<picturemarkersymbol url="assets/images/icon-m.png" height="30" width="30" />
    	</symbol>
    	<symbol keyvalue="m" alias="M板">
    		<picturemarkersymbol url="assets/images/icon-m.png" height="30" width="30" />
    	</symbol>
    	<symbol keyvalue="l" alias="L板">
    		<picturemarkersymbol url="assets/images/icon-l.png" height="30" width="30" />
    	</symbol>
    	<symbol keyvalue="a" alias="A板">
    		<picturemarkersymbol url="assets/images/icon-a.png" height="30" width="30" />
    	</symbol>
    	<symbol keyvalue="c" alias="C板">
    		<picturemarkersymbol url="assets/images/icon-c.png" height="30" width="30" />
    	</symbol>
    </symbols>
</configuration>
