<?xml version="1.0" ?>
<configuration>	
    <layer>http://[serverip]/arcgis/rest/services/changsha/changsha_device/MapServer/2</layer>
    <titlefield>desc_</titlefield>
    <idfield>Id</idfield>
    <type>DZJC</type>
    <linkfield></linkfield>
    <fields all="false">
        <field name="desc_" alias="设备位置"/>
        <field name="Id" alias="设备信息"/>
    </fields>
    <buttons>
		<button>电子警察</button>
	</buttons>
    <refreshrate>0</refreshrate>
    <zoomscale>48000</zoomscale>
    <query>1=1</query>
    <info>widgets/InfoTemplates/InfoPopupWidget.swf</info>
    <keyfield></keyfield>
    <symbols useinranksymbolwidget="true">
    	<symbol keyvalue="default">
    		<picturemarkersymbol url="assets/images/dzjc.png" height="20" width="20" />
    	</symbol>      	
    </symbols>
</configuration>
