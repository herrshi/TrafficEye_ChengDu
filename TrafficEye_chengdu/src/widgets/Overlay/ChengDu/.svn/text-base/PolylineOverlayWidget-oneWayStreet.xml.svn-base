<?xml version="1.0" ?>
<configuration>
	<type>oneWayStreet</type>
	<!-- 组件打开时通知页面 -->
	<widgetrunnotify></widgetrunnotify>
	<!-- 组件关闭时通知页面 -->
	<widgetclosenotify></widgetclosenotify>
	
	<popup>
		<title>{id}</title>
		<fields>
			<field name="id" alias="编号" visible="false" />
			<field name="name" alias="名称" visible="true" />
		</fields>
		<showzoomtobutton>false</showzoomtobutton>
	</popup>
	
	<simplerenderer>
		<simplelinesymbol style="solid" color="0x0055BF" alpha="1" width="3"/>
	</simplerenderer>
	
	<drawsymbol>
		<simplelinesymbol style="solid" color="0x0055BF" alpha="1" width="3"/>
	</drawsymbol>
</configuration>