<?xml version="1.0" ?>
<configuration>
	<type>trafficSymbolShow</type>
	<!-- 组件打开时通知页面 -->
	<widgetrunnotify>onTrafficSymbolShowOverlayLoad</widgetrunnotify>
	<!-- 组件关闭时通知页面 -->
	<widgetclosenotify>onTrafficSymbolShowOverlayClose</widgetclosenotify>
	<popup>
		<title>{id}</title>
		<fields>
			<field name="id" alias="ID" visible="true" />
			<field name="code" alias="编号" visible="true" />
			<field name="roadName" alias="路口名称" visible="true" />
			<field name="roadNature" alias="标志类型" visible="true" />
			<field name="belongArea" alias="所属区域" visible="true" />
			<field name="belongSuboffice" alias="所属分局" visible="true" />
		</fields>
		<showzoomtobutton>false</showzoomtobutton>
	</popup>
    <simplerenderer>
        <picturemarkersymbol url="assets/images/mapIcons/ShiGong-red.png" />
    </simplerenderer>
</configuration>